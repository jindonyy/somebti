    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "stylelint": "^16.7.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
